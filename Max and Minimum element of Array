#include<stdio.h>
int main()
{
        int i,m,max,min;
        printf("Enter the no of elements\n");
        scanf("%d",&m);
        int ar[m];
        printf("Enter the Elements of Array\n");
        for(i=0;i<m;i++)
        scanf("%d",&ar[i]);
        max=ar[0];
        min=ar[0];
        for(i=0;i<m;i++)
        {
                if(ar[i]>max)
                max=ar[i];
                if(ar[i]<min)
                min=ar[i];
        }
        printf("The Maximum element is %d\n",max);
        printf("The Minimum element is %d\n",min);
        return 0;
}
