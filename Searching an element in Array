#include<stdio.h>
int main()
{
        int i,m,n,pos,temp;
        printf("Enter the no of elements\n");
        scanf("%d",&m);
        int ar[m];
        printf("Enter the Elements of Array\n");
        for(i=0;i<m;i++)
        scanf("%d",&ar[i]);
        printf("Enter the element to be searched\n");
        scanf("%d",&n);
        for(i=0;i<m;i++)
        {
                if(ar[i]==n)
                {
                        pos=i;
                        temp=1;
                        break;
                }
                else
                temp=0;

        }
        if(temp==1)
        printf("Position of element : %d",pos);
        else
        printf("-1");
        }
